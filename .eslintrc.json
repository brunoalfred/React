{
    "extends": ["eslint:recommended", "prettier", "prettier/react"],
    "rules": {"react-hooks/rules-of-hooks": "error"},
    "plugins": ["react-hooks"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }